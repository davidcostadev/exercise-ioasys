{
  "name": "exercise-ioasys",
  "version": "1.0.0",
  "description": "exercise ioasys",
  "main": "src/main.js",
  "author": "David Costa <davidcostadev@gmail.com>",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "dev": "env-cmd nodemon src/main",
    "start": "env-cmd node src/main",
    "test": "cross-env NODE_ENV=test start-server-and-test start http://localhost:3000 cy:run",
    "jest": "jest",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "sequelize": "sequelize",
    "db:create": "sequelize db:create",
    "db:migrate": "sequelize db:migrate",
    "db:seed": "sequelize db:seed:all",
    "pretest:sequelize": "cross-env NODE_ENV=test npm run sequelize",
    "pretest:db:create": "cross-env NODE_ENV=test npm run db:create",
    "pretest:db:migrate": "cross-env NODE_ENV=test npm run db:migrate",
    "pretest:db:seed": "cross-env NODE_ENV=test npm run db:seed",
    "pretest": "npm run lint && npm run pretest:db:create && npm run pretest:db:migrate && npm run pretest:db:seed",
    "lint:format": "prettier --check '**/*.{js,json}'",
    "lint:js": "eslint src/** tests/**",
    "lint": "npm run lint:format && npm run lint:js"
  },
  "devDependencies": {
    "cross-env": "^7.0.2",
    "cypress": "^4.1.0",
    "env-cmd": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^25.1.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "start-server-and-test": "^1.10.8"
  },
  "dependencies": {
    "bcrypt": "^3.0.8",
    "body-parser": "^1.19.0",
    "expect": "^25.1.0",
    "express": "^4.17.1",
    "faker": "^4.1.0",
    "jsonwebtoken": "^8.5.1",
    "lodash.omit": "^4.5.0",
    "morgan": "^1.9.1",
    "pg": "^7.18.2",
    "sequelize": "^5.21.5",
    "sequelize-cli": "^5.5.1"
  }
}
